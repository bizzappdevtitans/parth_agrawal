<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Scheduled action for import projects from clickup backend model -->
    <record id="ir_cron_for_import_clickup_project_changes" model="ir.cron">
        <field name="name">Cron Import Clickup Project Changes</field>
        <field name="model_id" ref="model_clickup_backend" />
        <field name="type">ir.actions.server</field>
        <field name="state">code</field>
        <field name="code">model.cron_import_clickup_project_changes()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="False" />
        <field
            name="nextcall"
            eval="(DateTime.now().replace(hour=2, minute=30, second=0, microsecond=0, tzinfo=pytz.timezone('Asia/Kolkata')) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"
        />
    </record>
