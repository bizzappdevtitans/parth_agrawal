<?xml version="1.0" encoding="utf-8"?>
<odoo>

      <data>
        <record id="student_sequence" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">student.profile</field>
            <field name="prefix">ST</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

     <data>
        <record id="marksheet_sequence" model="ir.sequence">
            <field name="name">Marksheet Sequence</field>
            <field name="code">marksheet.option</field>
            <field name="prefix">MRS</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

     <data>
        <record id="fees_structure_sequence" model="ir.sequence">
            <field name="name">Fees Option</field>
            <field name="code">fees.option</field>
            <field name="prefix">REC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>


</odoo>
